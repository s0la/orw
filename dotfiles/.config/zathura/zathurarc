set recolor   
set recolor-lightcolor          "#1b1a1c"
set recolor-darkcolor           "#b3acbf"
set recolor-keephue

set default-fg                  "#b3acbf"
set default-bg                  "#1b1a1c"

set completion-bg               "#1b1a1c"
set completion-fg               "#b3acbf"
set completion-highlight-bg     ""
set completion-highlight-fg     "#1b1a1c"
set completion-group-bg         "#1b1a1c"
set completion-group-fg         "#b3acbf"

set statusbar-fg                "#4d4d4d"
set statusbar-bg                "#272628"
